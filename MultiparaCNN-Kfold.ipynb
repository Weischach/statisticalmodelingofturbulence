   "source": [
    "# Appropriate loss dunction can be slected for training. \n",
    "\n",
    "for train_index, test_index in KFold(n_splits=5).split(dfin):\n",
    "    train_X, test_X = dfin[train_index][:][:], dfin[test_index][:][:]\n",
